{
  "name": "Cheminot",
  "id": "m.cheminot",
  "description": "SNCF TER",
  "version": "0.0.0",
  "platforms": [
    "web",
    "android",
    "ios"
  ],
  "plugins": [
    "org.apache.cordova.splashscreen",
    "m.cheminot.plugin",
    "org.apache.cordova.plugin.softkeyboard",
    "org.apache.cordova.labs.keyboard"
  ],
  "configurations": {
    "web": {
      "default": {
        "id": "m.cheminot",
        "product_name": "Cheminot",
        "product_file_name": "Cheminot"
      },
      "dev": {
        "id": "m.cheminot",
        "product_name": "Cheminot dev",
        "product_file_name": "Cheminot"
      },
      "stage": {
        "id": "m.cheminot",
        "product_name": "Cheminot stage",
        "product_file_name": "Cheminot"
      },
      "prod": {
        "id": "m.cheminot",
        "product_name": "Cheminot prod",
        "product_file_name": "Cheminot"
      }
    },
    "android": {
      "default": {
        "id": "m.cheminot",
        "product_name": "Cheminot",
        "product_file_name": "Cheminot"
      },
      "dev": {
        "id": "m.cheminot.dev",
        "product_name": "Cheminot dev",
        "product_file_name": "Cheminot-dev"
      },
      "stage": {
        "id": "m.cheminot.stage",
        "product_name": "Cheminot stage",
        "product_file_name": "Cheminot-stage"
      },
      "prod": {
        "id": "m.cheminot.prod",
        "product_name": "Cheminot prod",
        "product_file_name": "Cheminot-prod"
      }
    },
    "ios": {
      "default": {
        "id": "m.cheminot",
        "product_name": "Cheminot",
        "product_file_name": "Cheminot"
      },
      "dev": {
        "id": "m.cheminot.dev",
        "product_name": "Cheminot dev",
        "product_file_name": "Cheminot-dev"
      },
      "stage": {
        "id": "m.cheminot.stage",
        "product_name": "Cheminot stage",
        "product_file_name": "Cheminot-stage"
      },
      "prod": {
        "id": "m.cheminot.prod",
        "product_name": "Cheminot prod",
        "product_file_name": "Cheminot-prod"
      }
    }
  },
  "cordova": {
    "preferences": {
      "DisallowOverscroll": true,
      "EnableViewportScale": false,
      "KeyboardDisplayRequiresUserAction": false,
      "ShowSplashScreenSpinner": false,
      "HideKeyboardFormAccessoryBar": false,
      "SplashScreen": "screen",
      "AutoHideSplashScreen": true,
      "KeyboardShrinksView": true,
      "KeepRunning": true
    },
    "accessOrigin": [
      "*"
    ]
  },
  "author": {
    "name": "Sebastien Renault",
